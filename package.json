    "@react-navigation/native-stack": "^7.8.5",
    "@react-navigation/stack": "^7.6.11",
    "@reduxjs/toolkit": "^2.6.1",
    "@testing-library/react-native": "^12.8.1",
    "expo": "^54.0.27",
