        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.thekian.plantfriends"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ddf0513e-726f-4f52-a386-4c770608b320"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}
